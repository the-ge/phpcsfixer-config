{
    "name": "thege/thege-phpcsfixer-config",
    "description": "The Ge's PHP CS Fixer configuration",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Gabriel Tenita",
            "email": "the.ge@tenita.eu"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3|^8.4|^8.5",
        "friendsofphp/php-cs-fixer": "^3",
        "kubawerlos/php-cs-fixer-custom-fixers": "dev-main"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
